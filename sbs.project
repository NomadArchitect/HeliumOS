
CC="i686-elf-gcc"
CXX="i686-elf-g++"
AS="nasm"
LINKER="i686-elf-g++"
STATIC_LINKER="i686-elf-ar"
CFLAGS="-std=gnu17"
CXXFLAGS="-std=gnu++20 -fno-exceptions -fno-rtti -fno-use-cxa-atexit"

{
  "configurations": [
    {
      "name": "HeliumOS",
      "includePath": [
        "${workspaceFolder}/kernel/include",
        "${workspaceFolder}/kernel/stdinc",
        "${workspaceFolder}/ext/src/dist",
        "${workspaceFolder}/sysroots/build/include",
        "${workspaceFolder}/sysroots/build/lib/gcc/x86_64-elf/11.2.0/include",
        "${workspaceFolder}/sysroots/build/lib/gcc/x86_64-elf/11.2.0/include-fixed",
        "${workspaceFolder}/sysroots/build/lib/gcc/x86_64-elf/11.2.0/plugin/include"
      ],
      "defines": ["__STDC_HOSTED__=0"],
      "compilerPath": "${workspaceFolder}/sysroots/build/bin/x86_64-elf-gcc",
      "compilerArgs": [
        "-mno-red-zone",
        "-Wall",
        "-fpic",
        "-ffreestanding",
        "-fno-stack-protector",
		    "-nostdlib",
        "-Werror",
        "-fno-asynchronous-unwind-tables"
      ],
      "cStandard": "c23",
      "cppStandard": "c++23",
      "intelliSenseMode": "gcc-x64"
    },
    {
      "name": "Linux",
      "cStandard": "c23",
      "cppStandard": "c++23",
      "intelliSenseMode": "linux-gcc-x64"
    }
  ],
  "version": 4
}